{
  "openapi": "3.0.2",
  "info": {
    "title": "FastAPI",
    "version": "0.1.0"
  },
  "paths": {
    "/health_check": {
      "get": {
        "summary": "Health Check",
        "operationId": "health_check_health_check_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HealthStatusResponse"
                }
              }
            }
          }
        }
      }
    },
    "/meta/": {
      "get": {
        "summary": "Available Model",
        "operationId": "available_model_meta__get",
        "parameters": [
          {
            "required": false,
            "schema": {
              "title": "Model Name",
              "type": "string"
            },
            "name": "model_name",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Version",
              "type": "string"
            },
            "name": "version",
            "in": "query"
          }
        ],
        "responses": {
          "404": {
            "description": "Model not found.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ModelNotFoundResponse"
                }
              }
            }
          },
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ModelsMetaDataResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/predict/": {
      "post": {
        "summary": "Predict",
        "operationId": "predict_predict__post",
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Input"
            },
            "name": "input",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Model Name",
              "type": "string"
            },
            "name": "model_name",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Version",
              "type": "string"
            },
            "name": "version",
            "in": "query"
          }
        ],
        "responses": {
          "404": {
            "description": "Model not found.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ModelNotFoundResponse"
                }
              }
            }
          },
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LabelScoreResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/predict/simple_text": {
      "post": {
        "tags": [
          "simple_text"
        ],
        "summary": "Predict Newest Version",
        "operationId": "predict_newest_version_predict_simple_text_post",
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Input"
            },
            "name": "input",
            "in": "query"
          }
        ],
        "responses": {
          "404": {
            "description": "Model not found.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ModelNotFoundResponse"
                }
              }
            }
          },
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LabelScoreResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/predict/simple_text/1.0.1": {
      "post": {
        "tags": [
          "simple_text"
        ],
        "summary": "Predict With Specific Version",
        "operationId": "predict_with_specific_version_predict_simple_text_1_0_1_post",
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Input"
            },
            "name": "input",
            "in": "query"
          }
        ],
        "responses": {
          "404": {
            "description": "Model not found.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ModelNotFoundResponse"
                }
              }
            }
          },
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LabelScoreResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "HTTPValidationError": {
        "title": "HTTPValidationError",
        "type": "object",
        "properties": {
          "detail": {
            "title": "Detail",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            }
          }
        }
      },
      "HealthStatusResponse": {
        "title": "HealthStatusResponse",
        "required": [
          "version",
          "generated_at"
        ],
        "type": "object",
        "properties": {
          "version": {
            "title": "Version",
            "type": "string"
          },
          "generated_at": {
            "title": "Generated At",
            "type": "string"
          }
        }
      },
      "LabelScoreResponse": {
        "title": "LabelScoreResponse",
        "required": [
          "preds",
          "pred_probas"
        ],
        "type": "object",
        "properties": {
          "preds": {
            "title": "Preds",
            "type": "array",
            "items": {
              "anyOf": [
                {
                  "type": "integer"
                },
                {
                  "type": "string"
                }
              ]
            }
          },
          "pred_probas": {
            "title": "Pred Probas",
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "array",
                "items": [
                  {
                    "anyOf": [
                      {
                        "type": "integer"
                      },
                      {
                        "type": "string"
                      }
                    ]
                  },
                  {
                    "type": "number"
                  }
                ]
              }
            }
          }
        }
      },
      "ModelDescription": {
        "title": "ModelDescription",
        "required": [
          "model_name",
          "version",
          "train_date"
        ],
        "type": "object",
        "properties": {
          "model_name": {
            "title": "Model Name",
            "type": "string"
          },
          "version": {
            "title": "Version",
            "type": "string"
          },
          "train_date": {
            "title": "Train Date",
            "type": "string"
          }
        }
      },
      "ModelMetaDataResponse": {
        "title": "ModelMetaDataResponse",
        "required": [
          "status",
          "meta_data",
          "request_format",
          "response_format"
        ],
        "type": "object",
        "properties": {
          "status": {
            "title": "Status",
            "enum": [
              "NOT_LOADED",
              "LOADING",
              "IDLE",
              "PREDICTING"
            ],
            "type": "string"
          },
          "meta_data": {
            "$ref": "#/components/schemas/ModelDescription"
          },
          "request_format": {
            "title": "Request Format",
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "object"
              }
            ]
          },
          "response_format": {
            "title": "Response Format",
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "object"
              }
            ]
          }
        }
      },
      "ModelNotFoundResponse": {
        "title": "ModelNotFoundResponse",
        "required": [
          "error_message",
          "available_models"
        ],
        "type": "object",
        "properties": {
          "error_message": {
            "title": "Error Message",
            "type": "string"
          },
          "available_models": {
            "title": "Available Models",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ModelMetaDataResponse"
            }
          }
        }
      },
      "ModelsMetaDataResponse": {
        "title": "ModelsMetaDataResponse",
        "required": [
          "models"
        ],
        "type": "object",
        "properties": {
          "models": {
            "title": "Models",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ModelMetaDataResponse"
            }
          }
        }
      },
      "ValidationError": {
        "title": "ValidationError",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "type": "object",
        "properties": {
          "loc": {
            "title": "Location",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        }
      }
    }
  }
}